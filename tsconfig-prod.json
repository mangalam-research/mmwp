{
  "extends": "./tsconfig-common.json",
  "compilerOptions": {
    "rootDirs": ["build/aot", "src"],
    "baseUrl": ".",
    "paths": {
      "wed": ["node_modules/wed/standalone/lib/wed"],
      "wed/*": ["node_modules/wed/standalone/lib/wed/*"],
      "dashboard/*": ["node_modules/wed-demo/aot-compiled/dashboard/*"]
    },
    "noUnusedParameters": false
  },
  "include": [
    "src/**/*.ts",
    "./build/aot/**/*.ts",
    "./build/aot/node_modules/**/*.ts"
  ]
}
